#ifndef TOK
#define TOK(X)
#endif
#ifndef KEYWORD
#define KEYWORD(X, Y) TOK(kw_ ## X)
#endif
#ifndef DELIMITER
#define DELIMITER(X, Y) TOK(X)
#endif

TOK(unknown)
TOK(eof)

TOK(basic_identifier) // 15.4.2
TOK(extended_identifier) // 15.4.3
TOK(decimal_literal) //15.5.2
TOK(based_literal) //15.5.3
TOK(character_literal) //15.6
TOK(string_literal) //15.7
TOK(bit_string_literal) //15.8
TOK(comment) //15.9

// 15.3 delimiters
DELIMITER(ampersand, "&")
DELIMITER(tick, "'")
DELIMITER(left_parenthesis, "(")
DELIMITER(right_parenthesis, ")")
DELIMITER(asterisk, "*")
DELIMITER(plus, "+")
DELIMITER(comma, ",")
DELIMITER(minus, "-")
DELIMITER(dot, ".")
DELIMITER(slash, "/")
DELIMITER(colon, ":")
DELIMITER(semicolon, ";")
DELIMITER(less, "<")
DELIMITER(equal, "=")
DELIMITER(greater, ">")
DELIMITER(grave, "`")
DELIMITER(vertical_line, "|")
DELIMITER(left_square, "[")
DELIMITER(right_square, "]")
DELIMITER(question_mark, "?")
DELIMITER(at, "@")

//15.3 compound delimiters
DELIMITER(arrow, "=>")
DELIMITER(double_star, "**")
DELIMITER(variable_assignment, ":=")
DELIMITER(inequal, "/=")
DELIMITER(greater_equal, ">=")
DELIMITER(less_equal, "<=")
DELIMITER(box, "<>")
DELIMITER(condition_conversion, "??")
DELIMITER(matching_equal, "?=")
DELIMITER(matching_inequal, "?/=")
DELIMITER(matching_less, "?<")
DELIMITER(matching_less_equal, "?<=")
DELIMITER(matching_greater, "?>")
DELIMITER(matching_greate_equal, "?>=")
DELIMITER(double_less, "<<")
DELIMITER(double_greater, ">>")

